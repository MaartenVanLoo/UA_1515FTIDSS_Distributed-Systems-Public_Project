### Create Node0 [POST]
POST http://localhost:8081/ns/nodes
Content-Type: application/json

{
  "name": "Node0",
  "ip": "192.168.0.10"
}
> {%
    client.test("Created successfully", function() {
        client.assert(response.status === 201, "Response status is not 201\nExpected: 201\nActual: " + response.status + "\n");
        client.global.set("node0", response.body.link);
    });
%}
